{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "api/overwrites/**",
          "**/toc.yml",
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": ".",
        "dest": "uwp_toolkit_dotnet_toc"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": {
      "files": [
        "api/overwrites/**/*.md"
      ]
    },
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "apiPlatform": "dotnet",
      "ms.topic": "managed-reference",
      "ms.prod": "windows",
      "ms.technology": "windows-toolkit",
      "ms.author": "nikolame",
      "author": "dotnet-bot"
    },
    "fileMetadata": {
      "langs": {
        "api/**.yml": [
          "csharp",
          "vb",
          "fsharp",
          "cppcx",
          "cppwinrt",
          "cpp"
        ]
      }
    },
    "template": [],
    "dest": "uwp-toolkit-dotnet-api",
    "lruSize": 0,
    "markdownEngineName": "markdig"
  }
}
